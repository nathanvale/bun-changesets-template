{
  "name": "@template/api",
  "version": "0.1.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/nathanvale/bun-changesets-template.git",
    "directory": "packages/api"
  },
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./remote-cache": {
      "types": "./dist/remote-cache.d.ts",
      "import": "./dist/remote-cache.js"
    },
    "./jwt": {
      "types": "./dist/utils/jwt.d.ts",
      "import": "./dist/utils/jwt.js"
    }
  },
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "clean": "rm -rf dist .tsbuildinfo",
    "lint": "eslint . --cache --cache-location ../../.eslintcache/api",
    "prepublishOnly": "pnpm run build && test -d dist",
    "test": "vitest run",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "@types/node": "24.3.0"
  }
}