{
  "name": "@template/utils",
  "version": "0.1.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/nathanvale/bun-changesets-template.git",
    "directory": "packages/utils"
  },
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./number": {
      "types": "./dist/number-utils.d.ts",
      "import": "./dist/number-utils.js"
    },
    "./path": {
      "types": "./dist/path-utils.d.ts",
      "import": "./dist/path-utils.js"
    },
    "./test": {
      "types": "./dist/test-utils.d.ts",
      "import": "./dist/test-utils.js"
    }
  },
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "clean": "rm -rf dist .tsbuildinfo",
    "lint": "NODE_NO_WARNINGS=1 eslint .",
    "lint:fix": "NODE_NO_WARNINGS=1 eslint . --fix",
    "prepublishOnly": "pnpm run build && test -d dist",
    "test": "vitest run",
    "test:ci": "vitest run --bail 1",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {},
  "publishConfig": {
    "access": "public",
    "provenance": true
  }
}
